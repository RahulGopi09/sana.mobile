<Procedure
    title="Patient Registration"
    author="Sana"
    uuid="18785afb-6160-45a0-ac8c-1f5cbfaa6d91"
    version="1.0"
    class="Subject">
	<Page>
		<Element type="HIDDEN" id="0"
		    concept="SYSTEM ID"
			question="Enter Patient's Medical Record ID Number"
			default="U@DEV:0000:@DATETIME:"/>
	</Page>
	<Page>
		<Element type="ENTRY" id="1"
		    concept="FIRST NAME" 
		    required="true" 
		    helpText="Enter the patient's first name." 
		    question="Enter Patient's First Name:" 
		    answer=""/>
        <Element type="ENTRY" id="2"
		   concept="FAMILY NAME"
		   required="true" 
		   helpText="Enter the patient's last name." 
		   question="Enter Patient's Last Name:" answer=""/>
	</Page>
	<Page>
		<Element type="SELECT" id="gender" concept="GENDER"
				 question="Enter the patient gender:"
				 required="true"
				 helpText="Select the patient's gender."
				 answer=""
				 choices="Male;Female"
				 values="M;F"
			/>
	</Page>
    <Page>
    <Element type="DATE"
             id="dob"
             concept="DOB"
             required="true"
             helpText="Enter the patient's date of birth."
             question="Date of Birth"
             answer="" />
    </Page>
