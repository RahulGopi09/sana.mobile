            type="ENTRY"
		    concept="FAMILY NAME"
		    required="true"/>
	</Page>
	<Page>
		<Element type="SELECT" id="5" concept="GENDER"
				 question="Enter the patient gender:"
				 required="true"
				 helpText="Select the patient's gender."
				 choices="M;F"
				 values="M;F"
			/>
	</Page>
    <Page>
        <Element
            id="6"
            type="DATE"
            concept="DOB"
            required="true"
            question="Date of Birth"/>
    </Page>
