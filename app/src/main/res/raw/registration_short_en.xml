<Procedure
    title="Patient Registration: Short Form"
    author="Sana"
    uuid="18785afb-6160-45a0-ac8c-1f5cbfaa6d92"
    version="1.0"
    locale="en"
    class="Subject"
	onComplete="intent://org.sana.provider/core/procedure/5299055a-c5e6-47e7-b649-dc17dac6019b#Intent;scheme=content;action=org.sana.intent.action.RUN_PROCEDURE;type=vnd.android.cursor.item/org.sana.procedure;end">
	<Page>
		<Element type="HIDDEN" id="1"
		    concept="SYSTEM ID"
			question="Enter Patient's Medical Record ID Number"
			default="U@DEV:@NOW:"/>
	</Page>
	<Page>
		<Element type="ENTRY" id="2"
		    concept="FIRST NAME" 
		    required="true" 
		    helpText="Enter the patient's first name." 
		    question="Enter Patient's First Name:" 
		    answer=""/>
        <Element type="ENTRY" id="3"
		   concept="FAMILY NAME"
		   required="true" 
		   helpText="Enter the patient's last name." 
		   question="Enter Patient's Last Name:" answer=""/>
	</Page>
	<Page>
		<Element type="SELECT" id="4" concept="GENDER"
				 question="Enter the patient gender:"
				 required="true"
				 helpText="Select the patient's gender."
				 answer=""
				 choices="Male;Female"
				 values="M;F"
			/>
	</Page>
    <Page>
    <Element type="DATE"
             id="5"
             concept="DOB"
             required="true"
             helpText="Enter the patient's date of birth."
             question="Date of Birth"
             answer="" />
    </Page>
